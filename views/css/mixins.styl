vendor(prop, args)
	-webkit-{prop} args
	-moz-{prop} args
	-o-{prop} args
	-ms-{prop} args
	{prop} args

border-radius()
	vendor('border-radius', arguments)

box-shadow()
	vendor('box-shadow', arguments)

clearfix()
	&:before
	&:after
		content: ""
		display: table
	&:after
		clear: both
	zoom: 1 if support-for-ie